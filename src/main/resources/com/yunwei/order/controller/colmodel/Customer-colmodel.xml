<?xml version="1.0" encoding="UTF-8"?>
<column-models id="customer">
	<profile id="stock" default="true" >
		<column-model>
			<header>客户ID</header>
			<name>custId</name>
			<width>50</width>
			<align>center</align>
		</column-model>
		<column-model>
			<header>客户名称</header>
			<name>cust_name</name>
			<width>100</width>
			<align>center</align>
		</column-model>
		<column-model>
			<header>联系人</header>
			<name>contact_name</name>
			<width>50</width>
			<align>center</align>
		</column-model>
		<column-model>
			<header>联系电话</header>
			<name>telephone</name>
			<width>50</width>
			<align>center</align>
		</column-model>
		<column-model>
			<header>电子邮件</header>
			<name>email</name>
			<width>50</width>
			<align>center</align>
		</column-model>
		<column-model>
			<header>详细地址</header>
			<name>address</name>
			<width>50</width>
			<align>center</align>
		</column-model>
	</profile>
</column-models>